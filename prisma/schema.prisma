datasource db {
  provider = "sqlite"
  url      = "file:dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id         Int       @id @default(autoincrement())
  token      String
  sessions   Session? @relation(fields: [sessionsId], references: [id])
  sessionsId Int?
}

model Session {
  id        Int    @id @default(autoincrement())
  name      String
  startDate String
  endDate   String
  User      User[]
}
